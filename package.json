{
	"name": "svelteql-starter",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"quality": "npm run quality:eslint && npm run quality:prettier && npm run quality:ts",
		"quality:fix": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && npm run quality:eslint --fix",
		"quality:eslint": "eslint --ignore-path .gitignore .",
		"quality:prettier": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"quality:ts": "svelte-check --tsconfig ./tsconfig.json",
		"generate": "graphql-codegen"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^2.11.2",
		"@graphql-codegen/typescript": "^2.7.2",
		"@graphql-codegen/typescript-operations": "^2.5.2",
		"@graphql-codegen/typescript-resolvers": "^2.7.2",
		"@graphql-eslint/eslint-plugin": "^3.10.6",
		"@graphql-tools/load-files": "^6.6.0",
		"@graphql-tools/merge": "^8.3.0",
		"@graphql-tools/schema": "^8.5.0",
		"@kitql/vite-plugin-watch-and-run": "^0.4.2",
		"@lucia-sveltekit/adapter-prisma": "^0.1.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"graphql": "^16.5.0",
		"graphql-codegen-svelte-apollo": "^1.1.0",
		"graphql-config": "^4.3.2",
		"graphql-scalars": "^1.17.0",
		"graphql-tag": "^2.12.6",
		"lucia-sveltekit": "^0.3.4",
		"postcss": "^8.4.14",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"prisma": "^4.1.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"uuid": "^8.3.2",
		"vite": "^3.0.0"
	},
	"type": "module",
	"dependencies": {
		"@apollo/client": "^3.6.9",
		"@envelop/depth-limit": "^1.6.1",
		"@envelop/generic-auth": "^4.3.1",
		"@graphql-yoga/common": "^2.12.3",
		"@prisma/client": "^4.1.0",
		"svelte-apollo": "^0.5.0"
	}
}
